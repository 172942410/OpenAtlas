#!/bin/bash 
if [[ $# < 1 || $1 == "--help" ]]
then
echo "Usage: GenerateCubesFromLabels.sh AtlasConfigFile"
echo "       Generates cube models( .vtk) for each label in the atlas"
exit 1
fi

Prog=GenerateCubesFromLabels
AtlasConfig=$1

AppDir=""
if [ `uname` == "Darwin" ]; then AppDir=$Prog.app/Contents/MacOS/ ; fi

@OpenAtlas_BINARY_DIR@/$AppDir/$Prog $AtlasConfig 1 5000
